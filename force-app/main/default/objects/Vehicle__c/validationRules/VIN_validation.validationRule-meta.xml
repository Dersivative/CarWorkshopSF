<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VIN_validation</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(Name) &lt;&gt; 17</errorConditionFormula>
    <errorMessage>VIN number must be exactly 17 characters.</errorMessage>
</ValidationRule>
